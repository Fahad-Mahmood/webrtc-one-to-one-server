{
    "env": {
      "es6": true,
      "node": true,
      "es2021": true
    },
    "parser": "@typescript-eslint/parser",
    "extends": [
      "airbnb-base",
      "plugin:@typescript-eslint/recommended",
      "plugin:json/recommended",
      "plugin:import/typescript"
    ],
    "plugins": ["@typescript-eslint"],
    "rules": {
      "@typescript-eslint/ban-ts-comment": ["off"],
      "no-console": ["off"],
      "import/extensions": ["off"]
    }
  }